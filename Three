#include <stdio.h>

int main() {
    int a;
    printf("Введіть номер маршруту: ");
    scanf("%d", &a);

    switch (a) {
        case 1:
            printf("Початкова зупинка : Парк Сонячний " "\nКінцева зупинка : Площа Пролетарської Перемоги 1");
            break;
        case 2:
            printf("Початкова зупинка : Синя Акація " "\nКінцева зупинка : Весела Поляна 2");
            break;
        case 3:
            printf("Початкова зупинка : Берізовий Гай " "\nКінцева зупинка : Край Надії  3");
            break;
        case 4:
            printf("Початкова зупинка : Вітряний Міст " "\nКінцева зупинка : ружба Народів  4");
            break;
        case 5:
            printf("Початкова зупинка : Радісний Перехрестя " "\nКінцева зупинка : Мирний Закуток  5");
            break;
        case 6:
            printf("Початкова зупинка : Золота Осінь " "\nКінцева зупинка : Весняний Сад 6");
            break;
        case 7:
            printf("Початкова зупинка : Зелений Берег " "\nКінцева зупинка : Срібний Водоспад  7");
            break;
        case 8:
            printf("Початкова зупинка : Літній Бриз " "\nКінцева зупинка : Казковий Ліс  8");
            break;          
        case 9:
            printf("Початкова зупинка : Мрійливий Рай " "\nКінцева зупинка : Весела Гавань 9");
            break;
        case 10:
            printf("Початкова зупинка : Сонячний Промінь " "\nКінцева зупинка : Барвистий Ранок  10");
            break;
        case 11:
            printf("Початкова зупинка : Мандруюча Зірка " "\nКінцева зупинка : Золотий Сузір'я  11");
            break;
        default:
            printf("Маршрут не знайдено.\n");
    }

    return 0;
